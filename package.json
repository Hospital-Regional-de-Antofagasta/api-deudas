  "bugs": {
    "url": "https://github.com/Hospital-Regional-de-Antofagasta/api-deudas/issues"
  },
  "homepage": "https://github.com/Hospital-Regional-de-Antofagasta/api-deudas#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.18",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
